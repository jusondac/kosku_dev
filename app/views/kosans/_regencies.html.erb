<% if @regencies.present? %>
  <div class="my-5" id="regencies_list">
    <%= form.collection_select :kabupaten_kota, @regencies, :id, :nama, {class: "border-none outline-none bg-gray-300 dark:bg-gray-800 px-3 py-2 shadow-2xl w-full", placeholder: 'Kabupaten/Kota'} %>
  </div>
  <% else %>
  <div class="my-5">
    Tidak ada bagian yang diupdate
  </div>
<% end %>
